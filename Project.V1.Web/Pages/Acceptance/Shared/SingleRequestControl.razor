<style type="text/css">
    .form-group .required-field::after {
        content: '**';
        margin-right: 4px;
        font-size: 13px;
        color: red;
    }

    .required-field label {
        font-weight: 500 !important;
    }

    .custom-font {
        font-size: 13px;
        font-weight: 400;
        color: rgba(0,0,0,0.82);
        padding-top: 0px;
    }

    .validation-message {
        font-size: 13px;
        font-weight: 400;
    }

    #toast_type .e-toast-icon.e-icons {
        height: auto;
        font-size: 30px;
    }

    .toast-icons.e-success::before {
        content: "\e701";
    }

    .toast-icons.e-error::before {
        content: "\e700";
    }

    .toast-icons.e-info::before {
        content: "\e704";
    }

    .toast-icons.e-warning::before {
        content: "\e703";
    }

    #toast_types {
        text-align: center;
    }

    @@font-face {
        font-family: 'Toast_icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    .toast-icons {
        font-family: 'Toast_icons' !important;
        speak: none;
        font-size: 55px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .e-dialog .e-dlg-header {
        color: red;
        font-size: 13px;
        font-weight: normal;
    }

    .e-dialog .e-dlg-content {
        color: red;
        font-size: 10px;
        font-weight: normal;
        line-height: normal;
    }
</style>

@if (RequestModel != null)

@page "/acceptance/engineer/worklist/{id}"
@layout MainLayoutAcceptance

<PageTitlePat Title="SMP (Acceptance) - Action Request" />

<style type="text/css">
    .custom-font {
        font-size: 13px;
        font-weight: 400;
        color: rgba(0,0,0,0.42);
        padding-top: 0px;
    }

    .validation-message {
        font-size: 13px;
        font-weight: 400;
    }

    #toast_type .e-toast-icon.e-icons {
        height: auto;
        font-size: 30px;
    }

    .toast-icons.e-success::before {
        content: "\e701";
    }

    .toast-icons.e-error::before {
        content: "\e700";
    }

    .toast-icons.e-info::before {
        content: "\e704";
    }

    .toast-icons.e-warning::before {
        content: "\e703";
    }

    #toast_types {
        text-align: center;
    }

    @@font-face {
        font-family: 'Toast_icons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    .toast-icons {
        font-family: 'Toast_icons' !important;
        speak: none;
        font-size: 55px;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    textarea {
        height: 65px;
        resize: none !important;
    }

    .custom-label {
        color: rgba(0,0,0,0.84) !important;
        font-size: 13px !important;
    }

    .e-input[disabled], .e-input-group .e-input[disabled], .e-input-group.e-control-wrapper .e-input[disabled], .e-input-group.e-disabled, .e-input-group.e-control-wrapper.e-disabled, .e-float-input input[disabled], .e-float-input.e-control-wrapper input[disabled], .e-float-input textarea[disabled], .e-float-input.e-control-wrapper textarea[disabled], .e-float-input.e-disabled, .e-float-input.e-control-wrapper.e-disabled {
        -webkit-text-fill-color: rgba(0,0,0,0.82) !important;
    }
</style>

<div class="col-sm-12 m-0 p-0" style="background: url('images/celltower.jpg'); background-size: cover; background-position: center right;">
    <div class="display-4 text-white" style="padding: 4rem 2rem;">
        Site Acceptance Action
    </div>
</div>

<BreadcrumbAccess OnAuthenticationCheck="AuthenticationCheck" Paths="@Paths" />

@if (RequestModel == null)
{
    <PageLoading />
}
else
{
    <div class="col-md-12">
        <SfToast CssClass="@ToastCss" ID="toast_default" @ref="ToastObj" Timeout="10000" Icon="e-circle_info"
             ShowCloseButton="true" NewestOnTop="true" ShowProgressBar="true">
            <ToastPosition X="@ToastPosition"></ToastPosition>
        </SfToast>
    </div>
    <div class="col-md-12 row px-4" style="">
        @if (RequestModel.SSVReport != null)
        {
            <div class="col-md-3">
                <a target="_blank" href="Documents/@((RequestModel.SSVReportIsWaiver) ? "Waiver" : "SSV")/@RequestModel.SSVReport" class="btn btn-warning"><i class="fas @((RequestModel.SSVReportIsWaiver) ? "fa-envelope" : "fa-file-excel")"></i> @((RequestModel.SSVReportIsWaiver) ? "Waiver Email" : "SSV Report")</a>
            </div>
        }
        @if (RequestModel.BulkuploadPath != null)
        {
            <div class="col-md-3">
                <a target="_blank" href="Documents/Bulk/@RequestModel.BulkuploadPath" class="btn btn-warning"><i class="fas fa-file-download"></i> Batch Upload File</a>
            </div>
        }
        <div class="col-md-3">
            <label class="custom-label e-label-top d-block">Unique Id</label>
            <span class="custom-label e-label-top d-block">@RequestModel.UniqueId</span>
        </div>
        @if (RequestModel.BulkBatchNumber != null)
        {
            <div class="col-md-3">
                <label class="custom-label e-label-top d-block">Batch No.</label>
                <span class="custom-label e-label-top d-block">@RequestModel.BulkBatchNumber</span>
            </div>
        }
    </div>

    <EditForm Model="@RequestModel">
        <ObjectGraphDataAnnotationsValidator />
        <div class="col-md-12" style="">
            <div class="col-md 12 row">
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="TechTypeModel" DataSource="@TechTypes" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Technology" style="font-size: 10px;" @bind-Value="RequestModel.TechTypeId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                            <DropDownListEvents TValue="string" TItem="TechTypeModel" ValueChange="OnTechChange"></DropDownListEvents>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.TechTypeId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Site ID" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.SiteId)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.SiteId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Site Name" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.SiteName)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.SiteName)" />
                    </div>
                </div>

                @if (RequestModel.TechTypeId != TechTypes.FirstOrDefault(x => x.Name == "4G")?.Id)
                {
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="BSC/RNC" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.RNCBSC)">
                            </SfTextBox>
                            <ValidationMessage For="@(() => RequestModel.RNCBSC)" />
                        </div>
                    </div>
                }
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="RegionViewModel" DataSource="@Regions" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Region" style="font-size: 10px;" @bind-Value="RequestModel.RegionId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.RegionId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="SummerConfigModel" DataSource="@SummerConfigs" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Summer Config" style="font-size: 10px;" @bind-Value="RequestModel.SummerConfigId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.SummerConfigId)" />
                    </div>
                </div>
                @*<div class="col-md-3 mt-3">
                    <div class="form-group">
                    <SfTextBox Enabled="false" Placeholder="Frequency Band" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.FrequencyBand)">
                    </SfTextBox>
                    <ValidationMessage For="@(() => RequestModel.FrequencyBand)" />
                    </div>
                    </div>*@
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Software" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.SoftwareVersion)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.SoftwareVersion)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="SpectrumViewModel" DataSource="@Spectrums" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Spectrum/Tech Split" style="font-size: 10px;" @bind-Value="RequestModel.SpectrumId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.SpectrumId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Bandwidth (MHz)" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.Bandwidth)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.Bandwidth)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Latitude" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.Latitude)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.Latitude)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Longitude" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.Longitude)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.Longitude)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="AntennaMakeModel" DataSource="@AntennaMakes" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Antenna Type" style="font-size: 10px;" @bind-Value="RequestModel.AntennaMakeId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.AntennaMakeId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="AntennaTypeModel" DataSource="@AntennaTypes" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Antenna Type" style="font-size: 10px;" @bind-Value="RequestModel.AntennaTypeId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.AntennaTypeId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Antenna Height(M)" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.AntennaHeight)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.AntennaHeight)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Antenna Azimuth" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.AntennaAzimuth)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.AntennaAzimuth)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="M Tilt" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.MTilt)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.MTilt)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="E Tilt" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.ETilt)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.ETilt)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="BaseBandModel" DataSource="@Basebands" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Baseband" style="font-size: 10px;" @bind-Value="RequestModel.BasebandId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.BasebandId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="RRUTypeModel" DataSource="@RRUTypes" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="RRU Type" style="font-size: 10px;" @bind-Value="RequestModel.RRUTypeId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.RRUTypeId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Power (w)" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.Power)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.Power)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDropDownList Enabled="false" TItem="ProjectTypeModel" DataSource="@ProjectTypes" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                    Placeholder="Project Type" style="font-size: 10px;" @bind-Value="RequestModel.ProjectTypeId">
                            <DropDownListFieldSettings Text="Name" Value="Id"></DropDownListFieldSettings>
                        </SfDropDownList>
                        <ValidationMessage For="@(() => RequestModel.ProjectTypeId)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Project Year" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.ProjectYear)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.ProjectYear)" />
                    </div>
                </div>
                @if (RequestModel.TechTypeId == TechTypes.FirstOrDefault(x => x.Name == "4G")?.Id)
                {
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="RRU Power (w)" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.RRUPower)">
                            </SfTextBox>
                            <ValidationMessage For="@(() => RequestModel.RRUPower)" />
                        </div>
                    </div>
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="CSFB status GSM" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.CSFBStatusGSM)">
                            </SfTextBox>
                            <ValidationMessage For="@(() => RequestModel.CSFBStatusGSM)" />
                        </div>
                    </div>
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="CSFB Status WCDMA" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.CSFBStatusWCDMA)">
                            </SfTextBox>
                            <ValidationMessage For="@(() => RequestModel.CSFBStatusWCDMA)" />
                        </div>
                    </div>

                }
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="RET Configured" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.RETConfigured)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.RETConfigured)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfTextBox Enabled="false" Placeholder="Carrier Aggregation" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.CarrierAggregation)">
                        </SfTextBox>
                        <ValidationMessage For="@(() => RequestModel.CarrierAggregation)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDatePicker Enabled="false" Placeholder="Site Integrated Date" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.IntegratedDate)">
                        </SfDatePicker>
                        <ValidationMessage For="@(() => RequestModel.IntegratedDate)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDatePicker Enabled="false" Placeholder="Date Submitted" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.DateSubmitted)">
                        </SfDatePicker>
                        <ValidationMessage For="@(() => RequestModel.DateSubmitted)" />
                    </div>
                </div>
                <div class="col-md-3 mt-3">
                    <div class="form-group">
                        <SfDatePicker Enabled="false" Placeholder="Date Requested" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.DateCreated)">
                        </SfDatePicker>
                        <ValidationMessage For="@(() => RequestModel.DateCreated)" />
                    </div>
                </div>
                @if (EnableSpecial)
                {
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfDatePicker Enabled="@(EnableSpecial)" Placeholder="Date Accepted" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(RequestModel.EngineerAssigned.DateApproved)"
                                  Min='@MinDateTime' Max='@MaxDateTime'>
                            </SfDatePicker>
                            <ValidationMessage For="@(() => RequestModel.EngineerAssigned.DateApproved)" />
                        </div>
                    </div>
                }
                <div class="col-md-12 row">
                    <div class="py-2 bg-light mb-3 col-12"></div>
                    <h3 class="col-12">Requester</h3>

                    <div class="py-1 bg-light mb-3 col-12"></div>

                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="Name" Value="@RequestModel.Requester.Name" FloatLabelType="@FloatLabelType.Auto">
                            </SfTextBox>
                        </div>
                    </div>
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="Phone Number" Value="@RequestModel.Requester.Phone" FloatLabelType="@FloatLabelType.Auto">
                            </SfTextBox>
                        </div>
                    </div>
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="Email" Value="@RequestModel.Requester.Email" FloatLabelType="@FloatLabelType.Auto">
                            </SfTextBox>
                        </div>
                    </div>
                    <div class="col-md-3 mt-3">
                        <div class="form-group">
                            <SfTextBox Enabled="false" Placeholder="Vendor" Value="@RequestModel.Requester.Vendor.Name" FloatLabelType="@FloatLabelType.Auto">
                            </SfTextBox>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </EditForm>
    <EditForm Model="@Input" OnValidSubmit='HandleValidSubmit' class="">
        <ObjectGraphDataAnnotationsValidator />
        <div class="col-md-12 mb-3" style="">
            <div class="col-md 12 row">
                <div class="py-2 bg-light mb-3 col-12"></div>
                <h3 class="col-12">Engineer's Section</h3>

                <div class="py-1 bg-light mb-3 col-12"></div>
                <div class="col-md-4 mt-3">
                    <SfDropDownList TItem="ProjectStatus" DataSource="@ProjectStatuses" TValue="string" CssClass="" FloatLabelType="@FloatLabelType.Auto"
                                Placeholder="Project Status" style="font-size: 10px;" @bind-Value="Input.Status">
                        <DropDownListFieldSettings Text="Name" Value="Status"></DropDownListFieldSettings>
                    </SfDropDownList>
                    <ValidationMessage For="@(() => Input.Status)" />
                </div>
                @if (Input.Status == "Rejected")
                {
                    <div class="col-md-4 mt-3">
                        <div class="form-group">
                            <SfTextBox Multiline="true" Placeholder="Reason for Rejection" FloatLabelType="@FloatLabelType.Auto" @bind-Value="@(Input.Comment)">
                            </SfTextBox>
                            <ValidationMessage For="@(() => Input.Comment)" />
                        </div>
                    </div>
                    <div class="col-md-4 mt-3">
                        <div class="form-group">
                            <label for="filePath" class="col-form-label custom-font" style="margin-left: 10px;">Engineer Reject Report</label>
                            <SfUploader AllowMultiple="false" Multiple="false" AllowedExtensions=".xls, .xlsx" MinFileSize="1000" MaxFileSize=15000000 AutoUpload="true" CssClass=""
                                @ref="SF_Uploader" @bind-Value="Input.EngineerRejectReport">
                                <UploaderEvents OnClear="OnClear" OnRemove="OnRemove" ValueChange='e => OnFileUploadChange(e, "EReport")'></UploaderEvents>
                            </SfUploader>
                            <ValidationMessage For="@(() => Input.EngineerRejectReport)" />
                        </div>
                    </div>
                }
                <div class="col-md-12 mt-3">
                    <div class="form-group">
                        <div class="col-sm-12 p-0 mt-4">
                            <SfButton CssClass="px-2" type="submit" IsPrimary="true" IconCss="@BulkUploadIconCss"
                                  IconPosition="IconPosition.Right">@BtnText</SfButton>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </EditForm>
}
